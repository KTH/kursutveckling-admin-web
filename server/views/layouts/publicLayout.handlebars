<!DOCTYPE html>
<html lang={{lang}}>
  <head>
    <title>KTH | {{title}}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{description}}">
    <link rel="shortcut icon" href="//www.kth.se/polopoly_fs/1.871948.1550155901!/favicon.ico">
    <!--link rel="shortcut icon" id="favicon" href="//www.kth.se/img/icon/favicon.ico"-->
    <link rel="shortcut icon" id="favicon" href="/kursinfoadmin/kursutveckling/static/icon/favicon">

    {{#if canonicalUrl}}
      <link href="{{canonicalUrl}}" rel="canonical">
    {{/if}}
   
    {{prefixStyle '/static/kth-style/css/kth-bootstrap.css' 'styles' 'screen'}}
    {{prefixStyle '/static/app.css' 'styles' 'screen'}}

    {{{render 'styles'}}}

    {{prefixScript '/static/vendor.js' 'head-scripts'}}
    {{prefixScript '/static/browserConfig' 'head-scripts'}}

  
    {{#extend 'head-scripts'}}
      <!--script src="//www.kth.se/social/toolbar/widget.js"></script-->
    {{/extend}}

   
    {{{render 'head-scripts'}}}
  </head>
  <body>
      {{> kthHeader }}
      <div class="container start main">
        <div class="row"> 
          {{{body}}}
        </div>
      </div> 
    {{{render 'scripts'}}}
  </body>
</html>