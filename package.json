{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-admin-web",
  "version": "2.0.0",
  "description": "Course information page",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-admin-web.git"
  },
  "scripts": {
    "//": "Webpack installs css and javascript",
    "dev": "NODE_ENV=development gulp build && rm -rf dist && parcel build --no-minify ./public/js/app/app.jsx && npm run vendor && parcel watch ./public/js/app/app.jsx",
    "vendor": "parcel build  ./public/js/vendor.js",
    "test": "mocha test/unit/*.js",
    "codecheck": "standard",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production npm rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "NODE_ENV=development rm -rf dist && parcel build --no-minify ./public/js/app/app.jsx && npm run vendor && parcel watch ./public/js/app/app.jsx",
    "build-dev-old": "cross-env NODE_ENV=development rm -rf dist && parcel build ./public/js/vendor.js && parcel watch ./public/js/app/app.jsx",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
