{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-admin-web",
  "version": "4.0.0",
  "description": "Course development admin page to upload course analyses/memo as pdf and fetch data from ladok, kopps, and push it to kurutveckling-api",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-admin-web.git"
  },
  "scripts": {
    "//": "Webpack installs css and javascript",
    "preinstall": "npx npm-force-resolutions",
    "vendor": "parcel build ./public/js/vendor.js",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "test": "NODE_ENV=test jest test --passWithNoTests",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "NODE_ENV=development rimraf ./dist && parcel build ./public/js/app/app.jsx && parcel build ./public/js/vendor.js --public-url /kursinfoadmin/kursutveckling/static && parcel watch ./public/js/app/app.jsx --public-url /kursinfoadmin/kursutveckling/static",
    "build-dev-win": "cross-env NODE_ENV=development rimraf ./dist && parcel build ./public/js/app/app.jsx && parcel build ./public/js/vendor.js --public-url /kursinfoadmin/kursutveckling/static && parcel watch ./public/js/app/app.jsx --public-url /kursinfoadmin/kursutveckling/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "start-dev-win": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev-win\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "dependencies": {
    "@azure/storage-blob": "^12.5.0",
    "@kth/kth-node-passport-oidc": "^1.0.42",
    "@kth/kth-reactstrap": "^0.4.42",
    "applicationinsights": "^1.8.9",
    "axios": "^0.21.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-handlebars": "^5.3.2",
    "kth-node-access-log": "^0.2.4",
    "kth-node-api-call": "^3.0.16",
    "kth-node-configuration": "^2.0.5",
    "kth-node-express-routing": "^2.0.8",
    "kth-node-i18n": "^1.0.9",
    "kth-node-log": "^3.1.5",
    "kth-node-monitor": "^1.1.11",
    "kth-node-redis": "^3.1.25",
    "kth-node-response": "https://github.com/KTH/kth-node-response#v1.0.0",
    "kth-node-server": "^3.1.3",
    "kth-node-session": "^2.0.2",
    "kth-node-web-common": "^5.1.16",
    "kth-style": "^7.0.2",
    "mobx": "^5.15.6",
    "mobx-react": "^5.4.4",
    "mobx-utils": "^5.6.2",
    "passport": "^0.4.1",
    "popper.js": "^1.16.1",
    "query-string": "^7.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-extended": "^0.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.38.0"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "engines": {
    "node": "14.17.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
