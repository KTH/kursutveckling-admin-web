{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-admin-web",
  "version": "4.0.0",
  "description": "Course development admin page to upload course analyses/memo as pdf and fetch data from ladok, kopps, and push it to kurutveckling-api",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-admin-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker": "npm install --development && npm run build && npm prune --production",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\"",
    "prepare": "husky install",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "test": "cross-env NODE_ENV=test jest test --passWithNoTests"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@babel/runtime": "^7.28.4",
    "@kth/api-call": "^4.4.0",
    "@kth/appinsights": "^0.6.0",
    "@kth/cortina-block": "^7.1.1",
    "@kth/kth-node-passport-oidc": "^5.3.0",
    "@kth/kth-node-response": "^1.0.8",
    "@kth/kth-node-web-common": "^9.6.1",
    "@kth/kth-reactstrap": "^0.5.0",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.3.1",
    "@kth/server": "^4.1.0",
    "@kth/session": "^3.0.9",
    "@kth/style": "^1.13.0",
    "@kth/ug-rest-api-helper": "^1.0.26",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "axios": "^1.12.2",
    "body-parser": "^1.20.3",
    "bootstrap": "^5.3.8",
    "cacheable-lookup": "^7.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "express-handlebars": "^7.1.3",
    "express-session": "^1.18.2",
    "jquery": "^3.7.1",
    "jquery-hoverintent": "^1.10.2",
    "kth-node-access-log": "^0.2.10",
    "kth-node-configuration": "^2.1.0",
    "kth-node-express-routing": "^2.3.0",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.4.0",
    "kth-style": "^10.5.3",
    "os": "^0.1.2",
    "passport": "^0.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "reactstrap": "^8.10.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@kth/eslint-config-kth": "^3.6.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "ansi-regex": "^6.2.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "decache": "^4.6.2",
    "eslint": "^8.57.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.5.2",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.1.10",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^13.3.3",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "20"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "npx lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
}
