{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-admin-web",
  "version": "4.0.0",
  "description": "Course development admin page to upload course analyses/memo as pdf and fetch data from ladok, kopps, and push it to kurutveckling-api",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-admin-web.git"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker": "npm install --development && npm run build && npm prune --production",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "postinstall": "npm run mini-css-extract-plugin && bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "mini-css-extract-plugin": "yarn add mini-css-extract-plugin --no-lockfile --ignore-scripts",
    "prepare": "husky install",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "test": "cross-env NODE_ENV=test jest test --passWithNoTests"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.11.0",
    "@babel/runtime": "^7.18.9",
    "@kth/api-call": "^4.0.39",
    "@kth/kth-node-passport-oidc": "^5.0.0",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^7.0.12",
    "@kth/kth-reactstrap": "^0.4.54",
    "@kth/log": "^4.0.4",
    "@kth/monitor": "^3.0.25",
    "@kth/server": "^4.0.2",
    "@kth/session": "^3.0.3",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/sdk-trace-base": "^1.3.1",
    "applicationinsights": "^2.3.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "cacheable-lookup": "^6.0.4",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "jquery": "^3.6.0",
    "jquery-hoverintent": "^1.10.2",
    "kth-node-access-log": "^0.2.8",
    "kth-node-configuration": "^2.0.12",
    "kth-node-express-routing": "^2.0.15",
    "kth-node-i18n": "^1.0.14",
    "kth-node-redis": "^3.1.42",
    "kth-style": "7.0.29",
    "os": "^0.1.2",
    "passport": "^0.5.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "reactstrap": "^9.0.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@kth/eslint-config-kth": "^3.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^28.1.6",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "decache": "^4.6.1",
    "eslint": "^8.19.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": "16"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
